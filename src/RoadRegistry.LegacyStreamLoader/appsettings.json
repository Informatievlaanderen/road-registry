{
  "LocalLegacyStreamFile": "import-streams.json",

  "ConnectionStrings": {
    "StreamStore": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.RoadRegistry;Trusted_Connection=True;"
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "RoadRegistry": "Debug",
      "Aiv": "Debug",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "RoadRegistry": "Verbose",
        "Aiv": "Verbose",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {
      "Application": "RoadRegistry - Legacy Stream Loader"
    }
  }
}
