{
  "presets": [
    ["env", {
      "modules": "commonjs",
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 9"]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "syntax-object-rest-spread",
    "transform-vue-jsx",
    "transform-runtime",
    "array-includes"],
  "env": {
    "test": {
      "plugins": ["syntax-object-rest-spread", "transform-vue-jsx", "transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}
