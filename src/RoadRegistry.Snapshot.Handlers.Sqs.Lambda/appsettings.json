{
  "ConnectionStrings": {
    "Events": "",
    "Snapshots": ""
  },

  "DataDog": {
    "Debug": false,
    "Enabled": false,
    "ServiceName": "road-registry-snapshot-lambda"
  },

  "DetailUrl": "",

  "RetryPolicy": {
    "MaxRetryCount": 50,
    "StartingRetryDelaySeconds": 1
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "RoadRegistry": "Warning",
        "Be.Vlaanderen.Basisregisters": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Properties": {
      "Application": "RoadRegistry - SNAPSHOT Lambda"
    }
  },

  "SnapshotOptions": {
    "EventCount": 10
  },

  "TicketingService": {
    "InternalBaseUrl": ""
  },

  "BlobClientType": "",

  "S3BlobClientOptions": {
    "Buckets": {
      "SqsMessages": ""
    }
  },

  "DistributedS3CacheOptions": {
    "Bucket": "",
    "RootDir": ""
  },

  "EmailClientOptions": {
    "FromEmailAddress": null,
    "ExtractUploadFailed": null
  },

  "SlackSinkOptions": {
    "WebHookUrl": null,
    "CustomIcon": ":ghost:",
    "ShowDefaultAttachments": false,
    "ShowExceptionAttachments": true
  }
}
