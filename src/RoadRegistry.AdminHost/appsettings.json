{
  "ConnectionStrings": {
    "EditorProjections": "",
    "Events": "",
    "ProductProjections": "",
    "Snapshots": ""
  },

  "DataDog": {
    "Debug": false,
    "Enabled": false,
    "ServiceName": "road-registry-admin-host"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "RoadRegistry": "Information",
        "Be.Vlaanderen.Basisregisters": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        }
      }
    ],
    "Properties": {
      "Application": "RoadRegistry - Admin Host",
      "ContainerId": "REPLACE_CONTAINERID"
    }
  },

  "SnapshotOptions": {
    "EventCount": 10
  },

  "DistributedLock": {
    "Enabled": false
  },

  "DistributedS3CacheOptions": {
    "Bucket": "",
    "RootDir": ""
  }
}
