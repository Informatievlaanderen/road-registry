{
  "BaseUrl": "https://api.staging-basisregisters.vlaanderen",

  "DataDog": {
    "ServiceName": "RoadRegistry BackOffice"
  },

  "FeatureToggles": {
    "UseApiKeyAuthentication": true,
    "UseHealthChecks": false,
    "UseOvoCodeInChangeRoadNetwork": false,
    "UseSnapshotRebuild": false
  },

  "NisCodeServiceUrl": null,

  "ApiKeys": {
    "Road": []
  },

  "SnapshotOptions": {
    "EventCount": 10
  },

  "EmailClientOptions": {
    "ExtractUploadFailed": null
  },

  "WegsegmentNaamruimte": "https://data.vlaanderen.be/id/wegsegment",

  "JobsBucket": {
    "BucketName": "basisregisters-dev-road-jobs",
    "UrlExpirationInMinutes": 60
  },

  "OpenIdConnectOptions": {
    "Authority": "",
    "AuthorizationRedirectUri": "",
    "AuthorizationIssuer": "",
    "AuthorizationEndpoint": "",
    "ClientId": "",
    "ClientSecret": "",
    "TokenEndPoint": "",
    "JwtSharedSigningKey": "",
    "JwtIssuer": "",
    "JwtAudience": "",
    "JwtExpiresInMinutes": "",

    "UserInfoEndPoint": "",
    "EndSessionEndPoint": "",
    "JwksUri": "",
    "PostLogoutRedirectUri": ""
  },

  "OAuth2IntrospectionOptions": {
    "ClientId": null,
    "ClientSecret": null,
    "Authority": null,
    "IntrospectionEndpoint": null
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "RoadRegistry": "Warning",
        "Be.Vlaanderen.Basisregisters": "Warning",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        }
      }
    ],
    "Properties": {
      "Application": "RoadRegistry - BackOffice Api",
      "ContainerId": "REPLACE_CONTAINERID"
    }
  },

  "DistributedLock": {
    "Enabled": false
  },

  "SlackSinkOptions": {
    "CustomIcon": ":ghost:",
    "ShowDefaultAttachments": false,
    "ShowExceptionAttachments": true
  }
}
