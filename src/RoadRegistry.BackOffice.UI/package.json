{
  "name": "road-registry",
  "version": "2.0.0",
  "description": "Road Registry",
  "author": "Basisregisters.Vlaanderen",
  "license": "EUPL-1.2",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/vlaamseoverheid/road-registry.git"
  },
  "private": true,
  "scripts": {
    "build-index": "node_modules/.bin/elm make src/Main.elm --optimize --output=output/index.js",
    "build-activity": "node_modules/.bin/elm make src/Activity.elm --optimize --output=output/activity.js",
    "build-information": "node_modules/.bin/elm make src/Information.elm --optimize --output=output/information.js",
    "build-download-for-editor": "node_modules/.bin/elm make src/DownloadForEditor.elm --optimize --output=output/download-for-editor.js",
    "build-download-product": "node_modules/.bin/elm make src/DownloadProduct.elm --optimize --output=output/download-product.js",
    "build-upload": "node_modules/.bin/elm make src/Upload.elm --optimize --output=output/upload.js",
    "prebuild": "npm run build-index && npm run build-activity && npm run build-information && npm run build-download-for-editor && npm run build-download-product && npm run build-upload",
    "build": "node_modules/.bin/parcel build --out-dir wwwroot index.html activity.html information.html download-for-editor.html download-product.html upload.html",
    "live": "node_modules/.bin/parcel index.html activity.html information.html download-for-editor.html download-product.html upload.html"
  },
  "engines": {
    "node": ">= 8.16.0",
    "npm": ">= 6.9.0"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "elm": "^0.19.1-3",
    "elm-analyse": "^0.16.5",
    "elm-format": "^0.8.3",
    "elm-hot": "^1.1.4",
    "elm-live": "^4.0.2",
    "node-elm-compiler": "^5.0.4",
    "parcel-bundler": "^1.12.4",
    "npm": "^6.14.5"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "dependencies": {}
}
